<%- include("./common/header.ejs") %>
        <link rel="stylesheet" href="/css/choose.css">
        <div class="member_wrap">
            <div class="member_header">
                <h3>수북이 쌓이는 책</h3>
            </div>
            <div class="member_content">
                <div class="member_title">
                    <span>사용자를 선택하세요.</span>
                </div>
                <div class="member_list"></div>
            </div>
        </div>
        <section class="layer_modal">
            <div class="lock_wrap animate"><!-- 잠금해제 비밀번호 -->
                <form action="/auth/lock_choose" method="post" id="lock_form" onsubmit="return false;">
                    <input type="hidden" name="user" value="">
                    <div class="lock_form">
                        <span class="close">×</span>
                        <div class="lock_title">
                            <span>잠금해제 비밀번호</span>
                        </div>
                        <div class="lock_desc">
                            <span>계정 잠금설정된 사용자입니다.</span>
                        </div>
                        <div class="lock_password">
                            <input type="password" name="lock_password" placeholder="잠금해제 비밀번호" maxlength="4">
                        </div>
                        <div class="lock_button">
                            <button type="button" class="lockbtn">접속하기</button>
                        </div>
                    </div>
                </form>
            </div>
        </section>
        <script src="/js/choose.js"></script>
<%- include("./common/footer.ejs") %>
